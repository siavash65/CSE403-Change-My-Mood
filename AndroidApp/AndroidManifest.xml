<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cmm.view"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="8" />

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    
    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".CMMActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MoodPage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MoodPage" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MediaPage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MediaPage" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AboutUs"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.AboutUs" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ContactUs"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.ContactUs" />
            </intent-filter>
        </activity>
        <activity android:name="PictureActivity" >
        </activity>
    </application>

</manifest>
